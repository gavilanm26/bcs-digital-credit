{
  "name": "bcs-digital-credit",
  "version": "1.0.0",
  "description": "Automated software testing with Playwright",
  "main": "index.js",
  "scripts": {
    "regresion": "playwright test --config=playwright.config.ts --project=Iphone12 --headed --grep @regresionPreAprobado",
    "test:chromium": "playwright test --config=playwright.config.ts --project=Chromium --headed",
    "test:firefox": "playwright test --config=playwright.config.ts --project=Firefox --headed",
    "test:safari": "playwright test --config=playwright.config.ts --project=Webkit --headed",
    "test:iphone12": "playwright test --config=playwright.config.ts --project=Iphone12 --headed",
    "test:google-chrome": "playwright test --config=playwright.config.ts --project=Google-Chrome --headed",
    "test:edge": "playwright test --config=playwright.config.ts --project=Microsoft-Edge --headed"
  },
  "author": "Mauricio Gavilan",
  "license": "ISC",
  "dependencies": {
    "playwright": "^1.29.1",
    "prettier": "^2.8.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.29.2",
    "allure-playwright": "^2.0.0-beta.24"
  }
}
