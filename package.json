{
  "name": "bcs-digital-credit",
  "version": "1.0.0",
  "description": "Automated software testing with Playwright",
  "main": "index.js",
  "scripts": {
    "smock": "playwright test --config=playwright.config.ts --project=Google-Chrome --headed --grep @smock",
    "test:desktop-chromium": "playwright test --config=playwright.config.ts --project=Chromium --headed",
    "test:desktop-firefox": "playwright test --config=playwright.config.ts --project=Firefox --headed",
    "test:desktop-chrome": "playwright test --config=playwright.config.ts --project=Google-Chrome --headed",
    "test:desktop-safari": "playwright test --config=playwright.config.ts --project=Webkit --headed --grep @smock",
    "test:desktop-edge": "playwright test --config=playwright.config.ts --project=Microsoft-Edge --headed",
    "test:iphone-13-pro": "playwright test --config=playwright.config.ts --project=Iphone13pro --headed --grep @smock",
    "test:iPad": "playwright test --config=playwright.config.ts --project=iPad --headed --grep @smock",
    "test:S9": "playwright test --config=playwright.config.ts --project=S9 --headed"
  },
  "author": "Mauricio Gavilan",
  "license": "ISC",
  "dependencies": {
    "playwright": "^1.29.1",
    "prettier": "^2.8.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.29.2",
    "allure-playwright": "^2.0.0-beta.24"
  }
}
